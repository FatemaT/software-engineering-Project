CREATE TABLE `tourist_assist`.`MALLS` (
  `MALL_id` INT NOT NULL COMMENT '',
  `MALL_name` VARCHAR(45) NULL COMMENT '',
  `MALL_addr` VARCHAR(255) NULL COMMENT '',
  `MALL_dtls` VARCHAR(255) NULL COMMENT '',
  `MALL_cdt` VARCHAR(45) NULL COMMENT '',
  `MALL_ct` VARCHAR(45) NULL COMMENT '',
  `MALL_ct_id` VARCHAR(45) NOT NULL COMMENT '',
  `MALL_STORE` VARCHAR(45) NOT NULL COMMENT '',
  `MALL_BRAND` VARCHAR(45) NOT NULL COMMENT '',

  PRIMARY KEY (`MALL_ID`)  COMMENT '');
