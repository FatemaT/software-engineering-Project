CREATE TABLE `tourist_assist`.`HOTELS` (
  `HOTEL_id` INT NOT NULL COMMENT '',
  `HOTEL_name` VARCHAR(45) NULL COMMENT '',
  `HOTEL_addr` VARCHAR(255) NULL COMMENT '',
  `HOTEL_dtls` VARCHAR(255) NULL COMMENT '',
  `HOTEL_cdt` VARCHAR(45) NULL COMMENT '',
  `HOTEL_ct` VARCHAR(45) NULL COMMENT '',
  `HOTEL_ct_id` VARCHAR(45) NOT NULL COMMENT '',
  PRIMARY KEY (`HOTEL_ID`)  COMMENT '');
